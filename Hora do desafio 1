using System;
class Program {
  static void Main (){
    Console.Write("Informe o tempo de viagem(Horas): ");
    float tempo = float.Parse(Console.ReadLine());
    Console.Write("Informe a velocidade média: ");
    float velocidade = float.Parse(Console.ReadLine());
    float distancia = tempo * velocidade;
    double comb = distancia/12;
    comb = Math.Round(comb, 3);
    Console.WriteLine("O combustível gasto para a viagem foi de: " + comb);}}
