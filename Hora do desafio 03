using System;

class Program {
  public static void Main (string[] args) {

    Console.WriteLine("Quantos números você quer na sequência?");
    int n = int.Parse(Console.ReadLine());
    int i = 0;
    int num = 0;
    int num2 = 1;

    for(i=0; i<=n; i++){
      Console.Write(num + " " + num2 + " ");
      num = num + num2;
      num2 = num + num2;
    }
  }
}
